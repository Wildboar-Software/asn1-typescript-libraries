{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2019",
    "module": "esnext",
    "lib": ["es2019", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@wildboar/x500/*": ["libs/x500/*"],
      "@wildboar/ansi-x9-42/*": ["libs/ansi-x9-42/*"],
      "@wildboar/ansi-x9-62/*": ["libs/ansi-x9-62/*"],
      "@wildboar/cms/*": ["libs/cms/*"],
      "@wildboar/pkcs/*": ["libs/pkcs/*"],
      "@wildboar/qc/*": ["libs/qc/*"],
      "@wildboar/ocsp/*": ["libs/ocsp/*"],
      "@wildboar/tsp/*": ["libs/tsp/*"],
      "@wildboar/sio/*": ["libs/sio/*"],
      "@wildboar/spkac/*": ["libs/spkac/*"],
      "@wildboar/crmf/*": ["libs/crmf/*"],
      "@wildboar/ess/*": ["libs/ess/*"],
      "@wildboar/cmp": ["libs/cmp/*"],
      "@wildboar/kerberos5/*": ["libs/kerberos5/*"],
      "@wildboar/scvp/*": ["libs/scvp/*"],
      "@wildboar/tal/*": ["libs/tal/*"],
      "@wildboar/ldap/*": ["libs/ldap/*"],
      "@wildboar/cmip/*": ["libs/cmip/*"],
      "@wildboar/rose/*": ["libs/rose/*"],
      "@wildboar/acse/*": ["libs/acse/*"],
      "@wildboar/rtse/*": ["libs/rtse/*"],
      "@wildboar/ftam/*": ["libs/ftam/*"],
      "@wildboar/cbeff/*": ["libs/cbeff/*"],
      "@wildboar/iso19785/*": ["libs/iso19785/*"],
      "@wildboar/m3100/*": ["libs/m3100/*"],
      "@wildboar/cinf/*": ["libs/cinf/*"],
      "@wildboar/rinf/*": ["libs/rinf/*"],
      "@wildboar/isis/*": ["libs/isis/*"],
      "@wildboar/bip/*": ["libs/bip/*"],
      "@wildboar/cmail/*": ["libs/cmail/*"],
      "@wildboar/guls/*": ["libs/guls/*"],
      "@wildboar/ccr/*": ["libs/ccr/*"],
      "@wildboar/asn1soap/*": ["libs/asn1soap/*"],
      "@wildboar/fast-infoset/*": ["libs/fast-infoset/*"],
      "@wildboar/tcap": ["libs/tcap/src/index.ts"],
      "@wildboar/t38": ["libs/t38/src/index.ts"],
      "@wildboar/iso-9798-3-sasl": ["libs/iso-9798-3-sasl/src/index.ts"],
      "@wildboar/usm": ["libs/usm/src/index.ts"],
      "@wildboar/clearance-sponsor-attribute": [
        "libs/clearance-sponsor-attribute/src/index.ts"
      ],
      "@wildboar/device-owner-attribute": [
        "libs/device-owner-attribute/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
